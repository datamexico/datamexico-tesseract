<Schema name="datamexico">
  <Cube name="anuies_postgrade">
    <Table name="anuies_postgraduate_enrollment" primary_key="mun_id" />

    <DimensionUsage foreign_key="age" source="Age Group" name="Age Group" />
    <DimensionUsage foreign_key="sex" source="Sex" name="Sex" />
    <DimensionUsage foreign_key="type" source="Academic Degree" name="Academic Degree" />

    <Dimension name="Period" foreign_key="period">
      <Hierarchy name="Period">
        <Table name="anuies_postgraduate_enrollment" />
        <Level name="Period" key_column="period" key_type="text"/>
      </Hierarchy>
    </Dimension>

    <Dimension name="Institution" foreign_key="institution">
      <Hierarchy name="Institution" primary_key="work_center_id">
        <Table name="dim_work_centers"/>
        <Level name="Institution" key_column="work_center_id" name_column="work_center_name" key_type="text"/>
      </Hierarchy>
    </Dimension>

    <Dimension name="Career" foreign_key="program">
      <Hierarchy name="Career" primary_key="code">
        <Table name="dim_careers_anuies"/>
        <Level name="Career" key_column="code" name_column="name_en" key_type="nontext">
          <Property name="Career ES" column="name_es" caption_set="es"/>
        </Level>
      </Hierarchy>
    </Dimension>

    <Measure name="Value" column="value" aggregator="sum" />

  </Cube>
</Schema>