          <ColumnDef name="name_en" key_type="text" />
          <ColumnDef name="name_es" key_type="text" />
          <Row>
            <Value column="id">1</Value>
            <Value column="name_en">Employed workers</Value>
            <Value column="name_es">Personal ocupado</Value>
          </Row>
          <Row>
            <Value column="id">2</Value>
            <Value column="name_en">Personal dependent on the social reason</Value>
            <Value column="name_es">Personal dependiente de la razón social</Value>
          </Row>
          <Row>
            <Value column="id">3</Value>
            <Value column="name_en">Paid staff</Value>
            <Value column="name_es">Personal remunerado</Value>
          </Row>
          <Row>
            <Value column="id">4</Value>
            <Value column="name_en">Production personnel, sales and service</Value>
            <Value column="name_es">Personal de producción, ventas y servicios</Value>
          </Row>
          <Row>
            <Value column="id">5</Value>
            <Value column="name_en">Administrative, accounting and managerial staff</Value>
            <Value column="name_es">Personal administrativo, contable y de dirección</Value>
          </Row>
          <Row>
            <Value column="id">6</Value>
            <Value column="name_en">Owners, family and other unpaid workers</Value>
