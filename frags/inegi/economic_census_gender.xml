          </Row>
          <Row>
            <Value column="id">8</Value>
            <Value column="name_en">Personnel provided by another company name</Value>
            <Value column="name_es">Personal suministrado por otra razón social</Value>
          </Row>
          <Row>
            <Value column="id">9</Value>
            <Value column="name_en">Staff for fees or commissions without basic salary</Value>
            <Value column="name_es">Personal por honorarios o comisiones sin sueldo base</Value>
          </Row>
        </InlineTable>
        <Level name="Classification" key_column="id" name_column="name_en">
          <Property name="Classification ES" column="name_es" caption_set="es" />
        </Level>
      </Hierarchy>
    </Dimension>
    <DimensionUsage foreign_key="mun_id" source="Geography Municipality" name="Geography" />
    <DimensionUsage foreign_key="year" source="Year" name="Year" />
    <DimensionUsage foreign_key="sex" source="Sex" name="Sex" />
