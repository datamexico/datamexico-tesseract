<Schema name="datamexico">
  <Cube name="inegi_suicides_over_mortality">
    <Table name="inegi_suicides_over_mortality" primary_key="ent_id" />

    <DimensionUsage foreign_key="ent_id" source="Geography Municipality" name="Geography" />
    <DimensionUsage foreign_key="year" source="Year" name="Year" />

    <Measure name="Suicides" column="suicides" aggregator="sum" />
    <Measure name="Rate of Violent Deaths" column="rate_violent_deaths" aggregator="sum" />
    <Measure name="Overmortality Index" column="male_overmortality_index" aggregator="sum" />

  </Cube>
</Schema>