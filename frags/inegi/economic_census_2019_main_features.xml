<Schema name="datamexico" default_locale="en">
  <Cube name="inegi_economic_census_main_features">

    <Annotation name="topic">Economy</Annotation>
    <Annotation name="subtopic">Employment</Annotation>
    <Annotation name="source_name">Censos Econ√≥micos</Annotation>
    <Annotation name="source_name_en">Economic Censuses</Annotation>
    <Annotation name="source_link">https://www.inegi.org.mx/programas/ce/2019/#Tabulados</Annotation>

    <Table name="inegi_economic_census_main_features" primary_key="ent_id" />

    <DimensionUsage foreign_key="ent_id" source="Geography State Lite" name="Geography State" />
    <DimensionUsage foreign_key="mun_id" source="Geography Municipality Lite" name="Geography Municipality" />
    <DimensionUsage foreign_key="sector_id" source="FDI Industry 2 Digit" name="FDI Industry 2 Digit" />
    <DimensionUsage foreign_key="subsector_id" source="FDI Industry 3 Digit Lite" name="FDI Industry 3 Digit" />
    <DimensionUsage foreign_key="rama_id" source="FDI Industry 4 Digit Lite" name="FDI Industry 4 Digit" />
    <DimensionUsage foreign_key="year" source="Year" name="Year" />

    <Dimension name="Level" foreign_key="level">
      <Hierarchy name="Level" primary_key="id">
        <InlineTable alias="dim_inline_level">
          <ColumnDef name="id" key_type="nontext" />
          <ColumnDef name="name_en" key_type="text" />
          <ColumnDef name="name_es" key_type="text" />
          <Row>
            <Value column="id">1</Value>
            <Value column="name_en">State-Sector</Value>
            <Value column="name_es">Entidad-Sector</Value>
          </Row>
            <Row>
            <Value column="id">2</Value>
            <Value column="name_en">State-Subsector</Value>
            <Value column="name_es">Entidad-Subsector</Value>
          </Row>
            <Row>
            <Value column="id">3</Value>
            <Value column="name_en">State-Industry</Value>
            <Value column="name_es">Entidad-Rama</Value>
          </Row>
            <Row>
            <Value column="id">4</Value>
            <Value column="name_en">Municipality-Sector</Value>
            <Value column="name_es">Municipio-Sector</Value>
          </Row>
        </InlineTable>
        <Level name="Level" key_column="id" name_column="name_en">
          <Property name="Level ES" column="name_es" caption_set="es" />
        </Level>
      </Hierarchy>
    </Dimension>

    <Measure name="Economic Unit" column="UE" aggregator="sum" />
    <Measure name="Total Staff Occupied" column="total_personal_ocupado" aggregator="sum" />
    <Measure name="Total Staff Occupied Dependent" column="total_personal_ocupado_dependiente" aggregator="sum" />
    <Measure name="Paid Staff Occupied" column="personal_ocupado_remunerado" aggregator="sum" />
    <Measure name="Unremunerated Family Owners and Others" column="propietarios_familiares_otros_no_remunerados" aggregator="sum" />
    <Measure name="Not Dependent Business" column="no_dependiente_razon_social" aggregator="sum" />
    <Measure name="Remunerations" column="remuneraciones" aggregator="sum" />
    <Measure name="Expenditure Consumption Goods and Services" column="gasto_consumo_bienes_servicios" aggregator="sum" />
    <Measure name="Income Supply Good and Services" column="ingreso_suministro_bienes_servicios" aggregator="sum" />
    <Measure name="Total Gross Production" column="produccion_total_bruta" aggregator="sum" />
    <Measure name="Intermediate Consumption" column="consumo_intermedio" aggregator="sum" />
    <Measure name="Gross Census Value Added" column="valor_agregado_censal_bruto" aggregator="sum" />
    <Measure name="Gross Gixed Capital Formation" column="formacion_bruta_capital_fijo" aggregator="sum" />
    <Measure name="Total Stock Variation" column="variacion_total_existencias" aggregator="sum" />
    <Measure name="Fixed Assets" column="activos_fijos" aggregator="sum" />
    <Measure name="Fixed Assets Depreciation" column="depreciacion_activos_fijos" aggregator="sum" />
    <Measure name="Participation Remunerations Consumptions Goods and Services" column="participacion_remuneraciones_gastos_consumo_bienes_servicios" aggregator="sum" />
    <Measure name="Gross Census Value Added Total Gross Production" column="valor_agregado_censal_bruto_produccion_bruta_total" aggregator="sum" />
    <Measure name="Average Rate of Return" column="tasa_rentabilidad_promedio" aggregator="sum" />
    <Measure name="Total Gross Production Fixed Assets" column="activos_fijos_produccion_bruta_total" aggregator="sum" />
    <Measure name="Participation Depreciation Value Fixed Assets" column="participacion_depreciacion_valor_Activos_fijos" aggregator="sum" />
    <Measure name="Gross Operating Margin" column="margen_bruto_operacion" aggregator="sum" />
    <Measure name="Total Gross Production Share of Intermediate Consumption " column="participacion_consumo_intermedio_produccion_bruta_total" aggregator="sum" />
    <Measure name="Total Added Value Fixed Assets" column="valor_agregado_total_activos_fijos" aggregator="sum" />
    <Measure name="Average Added Value Employed Person" column="valor_agregado_promedio_persona_ocupada" aggregator="sum" />
    <Measure name="Total Gross Production Total Employed Personnel" column="produccion_bruta_total_personal_ocupado_total" aggregator="sum" />
    <Measure name="Average Remuneration Paid Person" column="remuneracion_media_persona_remunerada" aggregator="sum" />
    <Measure name="Value of Fixed Assets Employed Person" column="valor_activos_fijos_persona_ocupada" aggregator="sum" />

  </Cube>
</Schema>