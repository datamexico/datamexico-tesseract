<Schema name="datamexico" default_locale="en">
  <Cube name="inegi_economic_census_mun_ue">
    
    <Annotation name="topic">Economy</Annotation>
    <Annotation name="subtopic">Employment</Annotation>
    <Annotation name="source_name">Censos Econ√≥micos</Annotation>
    <Annotation name="source_name_en">Economic Censuses</Annotation>
    <Annotation name="source_link">https://www.inegi.org.mx/programas/ce/2019/</Annotation>

    <Table name="inegi_economic_census_mun_ue" primary_key="mun_id" />

    <DimensionUsage foreign_key="mun_id" source="Geography Municipality" name="Geography" />
    <DimensionUsage foreign_key="year" source="Year" name="Year" />
    <DimensionUsage foreign_key="sector_id" source="Industry 2 Digit" name="Economic Sector" />

    <Dimension name="Company Size" foreign_key="estrato_id">
      <Hierarchy name="Company Size" primary_key="id">
        <InlineTable alias="dim_inline_estrato">
          <ColumnDef name="id" key_type="nontext" />
          <ColumnDef name="name_en" key_type="text" />
          <ColumnDef name="name_es" key_type="text" />
          <Row>
            <Value column="id">1</Value>
            <Value column="name_en">0 - 10</Value>
            <Value column="name_es">0 - 10</Value>
          </Row>
          <Row>
            <Value column="id">2</Value>
            <Value column="name_en">11 - 50</Value>
            <Value column="name_es">11 - 50</Value>
          </Row>
          <Row>
            <Value column="id">3</Value>
            <Value column="name_en">51 - 250</Value>
            <Value column="name_es">51 - 250</Value>
          </Row>
          <Row>
            <Value column="id">4</Value>
            <Value column="name_en">251 +</Value>
            <Value column="name_es">251 +</Value>
          </Row>
        </InlineTable>
        <Level name="Company Size" key_column="id" name_column="name_en">
          <Property name="Company Size ES" column="name_es" caption_set="es" />
        </Level>
      </Hierarchy>
    </Dimension>

    <Measure name="Economic Unit" column="ue" aggregator="sum" >
        <Annotation name="details">Number Companies</Annotation> 
    </Measure>

  </Cube>
</Schema>