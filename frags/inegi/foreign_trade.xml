<Schema name="datamexico">
    <Cube name="foreign_trade">
        <Table name="foreign_trade" primary_key="date_id" />

        <Dimension name="Document Code" foreign_key="document_code">
            <Hierarchy name="Document Code">
                <Table name="foreign_trade" />
                <Level name="Document Code" key_column="document_code" />
            </Hierarchy>
        </Dimension>

        <Dimension name="Operation" foreign_key="operation_code">
            <Hierarchy name="Operation" primary_key="id">

                <InlineTable alias="dim_inline_coperation_code">
                    <ColumnDef name="id" key_type="nontext" />
                    <ColumnDef name="name_en" key_type="text" />
                    <ColumnDef name="name_es" key_type="text" />
                    <Row>
                        <Value column="id">1</Value>
                        <Value column="name_en">Imports</Value>
                        <Value column="name_es">Importacion</Value>
                    </Row>
                    <Row>
                        <Value column="id">2</Value>
                        <Value column="name_en">Exports</Value>
                        <Value column="name_es">Exportacion</Value>
                    </Row>
                </InlineTable>
                <Level name="Operation" key_column="id" name_column="name_en">
                    <Property name="Operation ES" column="name_es" caption_set="es"/>
                </Level>
            </Hierarchy>
        </Dimension>

        // codes range(1,32)
        <Dimension name="State Code Taxpayer" foreign_key="state_code_taxpayer">
            <Hierarchy name="State Code Taxpayer">
                <Table name="foreign_trade" />
                <Level name="State Code Taxpayer" key_column="state_code_taxpayer" />
            </Hierarchy>
        </Dimension>

        <Dimension name="Country Taxpayer" foreign_key="country_taxpayer">
            <Hierarchy name="Country Taxpayer">
                <Table name="foreign_trade" />
                <Level name="Country Taxpayer" key_column="country_taxpayer" />
            </Hierarchy>
        </Dimension>

        <Dimension name="Country Origin Destiny" foreign_key="country_origin_destiny">
            <Hierarchy name="Country Origin Destiny">
                <Table name="foreign_trade" />
                <Level name="Country Origin Destiny" key_column="country_origin_destiny" />
            </Hierarchy>
        </Dimension>

        <Dimension name="Commercial Measure Code" foreign_key="commercial_measure_code">
            <Hierarchy name="Commercial Measure Code">
                <Table name="foreign_trade" />
                <Level name="Commercial Measure Code" key_column="commercial_measure_code" />
            </Hierarchy>
        </Dimension>

        <Dimension name="Code of Entry Customs Section" foreign_key="code_of_entry_customs_section">
            <Hierarchy name="Code of Entry Customs Section">
                <Table name="foreign_trade" />
                <Level name="Code of Entry Customs Section" key_column="code_of_entry_customs_section" />
            </Hierarchy>
        </Dimension>

        <Dimension name="Postal Code Address" foreign_key="postal_code_taxpayer_address">
            <Hierarchy name="Postal Code Address">
                <Table name="foreign_trade" />
                <Level name="Postal Code Address" key_column="postal_code_taxpayer_address" />
            </Hierarchy>
        </Dimension>

        <Measure name="Tariff Fraction" column="tariff_fraction" aggregator="sum"/>
        <Measure name="Unitary Price" column="unitary_price" aggregator="sum" />
        <Measure name="Commercial Value" column="commercial_value" aggregator="sum"/>

    </Cube>
</Schema>