<Schema name="datamexico" default_locale="en">
  <Cube name="inegi_enigh_household_income">
    
      <Annotation name="topic">Housing and Living</Annotation>
      <Annotation name="subtopic">Income</Annotation>
      <Annotation name="source_name">Instituto Nacional de Estadística y Geografía (INEGI)</Annotation>
      <Annotation name="source_name_en">National Institute of Statistics and Geography (INEGI)</Annotation>
      <Annotation name="source_link">https://www.inegi.org.mx/programas/enigh/nc/2018/</Annotation>
    

    <Table name="inegi_enigh_household_income" primary_key="mun_id" />

    <Dimension name="Year" foreign_key="year">
      <Hierarchy name="Year">
        <Level name="Year" key_column="year" type="time" />
      </Hierarchy>
    </Dimension>

    <DimensionUsage foreign_key="mun_id" source="Geography Municipality" name="Geography" />
    <Measure name="Population" column="population" aggregator="sum">
      
        <Annotation name="details">Number of people</Annotation>
      
    </Measure>
    <Measure name="Monthly Average Income" column="monthly_average" aggregator="sum">
      
        <Annotation name="details">Value in mexican pesos</Annotation>
      
    </Measure>

  </Cube>
</Schema>