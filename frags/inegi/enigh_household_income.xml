<Schema name="datamexico">
  <Cube name="inegi_enigh_household_income">
    <Table name="inegi_enigh_household_income" primary_key="mun_id" />

    <Dimension name="Year" foreign_key="year">
      <Hierarchy name="Year">
        <Level name="Year" key_column="year" type="time" />
      </Hierarchy>
    </Dimension>

    <DimensionUsage foreign_key="mun_id" source="Geography Municipality" name="Geography" />
    <Measure name="Population" column="population" aggregator="sum" />
    <Measure name="Monthly Average Income" column="monthly_average" aggregator="sum"/>

  </Cube>
</Schema>